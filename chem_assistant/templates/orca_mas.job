#!/bin/bash
#SBATCH -J name
#SBATCH -e name.%j.err
#SBATCH -N 1
#SBATCH -n 16
#SBATCH -c 1
#SBATCH --mem=64G
#SBATCH -t 10:00:00
#SBATCH -p comp

export PROJECT="sn29"

module load orca/4.2.1
module load openmpi/1.10.7-mlx
orcapath=$(dirname $(which orca))

time $orcapath/orca name.inp > name.log

find . -empty -delete
