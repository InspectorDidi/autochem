#!/bin/bash
#SBATCH -J name
#SBATCH -e name.%j.err
#SBATCH -N 1
#SBATCH -n 16
#SBATCH -c 1
#SBATCH --mem=64G
#SBATCH -t 10:00:00
#SBATCH -p comp,short
#SBATCH -q partner

export PROJECT="p2015120004"

module load orca/4.0.1
module load openmpi/1.10.7-mlx
orcapath=$(dirname $(which orca))

$orcapath/orca name.inp > name.log

find . -empty -delete
